<div class="w3-center w3-padding-top-64">
  <h2 class="">Suscripciones Activas</h2>
  {{#subscription}}
    <form action="/Suscripcion/unregister" method="POST">
      <div class="w3-display-container ">
        <center>
          <ul class="w3-ul w3-card-4 w3-margin-left" style="width:50%">
            
            <li class="w3-margin w3-center">
            <img src="{{dailyImageUrl}}" alt="" width="120">
            <button class="w3-button w3-red w3-margin-left">Desuscribirse</button>
            </li>
              
            <input id="userId" name="userId" type="hidden" value="{{ID}}">
            <input id="dailyId" name="dailyId" type="hidden" value="{{dailyIdTable}}">

          </ul>
        </center>
      </div>
    </form>
  {{/subscription}}
</div>

{{/logueado}}
