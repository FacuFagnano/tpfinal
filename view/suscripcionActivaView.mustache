<form class="w3-container w3-center" action="/Suscripcion/unregister" method="POST">
  <div class="w3-row-padding">
    <div class="w3-center w3-col s4 w3-margin-bottom">
      <div class="w3-card-4">
        <img src="{{dailyImageUrl}}" alt="Clarin" width="300">

        <input id="userId" name="userId" type="hidden" value="{{ID}}">
        <input id="dailyId" name="dailyId" type="hidden" value="{{dailyIdTable}}">

        <div class="w3-container w3-center">
          <p>{{dailyDescription}}</p>
          <button class="w3-button w3-red w3-center w3-margin">Desuscribirse</button>
        </div>
      </div>

    </div>
  

  </div>

</form>
 {{/subscription}}
