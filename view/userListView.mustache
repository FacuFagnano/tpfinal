{{#logueado}}
  {{> headerLogueado}}

<div class="w3-container w3-white w3-margin w3-center">
          
            <h3>ADMIN</h3>
            
                      


     
<div class='w3-container w3-content w3-center' style='max-width:800px' id='userList'>
    <h2 class='w3-wide'>Usuario Registrados</h2>
    <table class='w3-table'>
        <tr>
            <th>Nombre</th>
            <th>Apellido</th>
            <th>Rol</th>
            <th>Accion</th>
            <th>Cambiar Rol</th>
        </tr>

        {{#user}}
           <tr>
                <td>{{NAME}}</td>
                <td>{{LASTNAME}}</td>
                <td>{{ROL}}</td>
                <div class="w3-justify">
                <td><button class="w3-button w3-round-xlarge">Eliminar</button></td>     
                <td><select class="w3-input w3-border">
                      <option value="3">Editor</option>
                  </select>
                </td>
                </div>
            </tr>
        {{/user}}

    </table>
</div>
          </div>
{{/logueado}}
{{^logueado}}
  {{> header}}


<!-- The App Section -->
<div class="w3-padding-64 w3-white">
  <div class="w3-row-padding">
    <div class="w3-col l8 m6">
     <h1 class="w3-xxxlarge w3-text-green"><b>No tienes permisos</b></h1>
      <button class="w3-button w3-light-grey w3-padding-large w3-section w3-hide-small" onclick="document.getElementById('download').style.display='block'">
        <i class="fa fa-download"></i> Download Application
      </button>
      <p>Available for <i class="fa fa-android w3-xlarge w3-text-green"></i> <i class="fa fa-apple w3-xlarge"></i> <i class="fa fa-windows w3-xlarge w3-text-blue"></i></p>
    </div>
    <div class="w3-col l4 m6">
      <img src="/public/w3images/bloqueo.jpg" class="w3-image w3-right w3-hide-small" width="335" height="471">
      <div class="w3-center w3-hide-large w3-hide-medium">
        <button class="w3-button w3-block w3-padding-large" onclick="document.getElementById('download').style.display='block'">
          <i class="fa fa-download"></i> Download Application
        </button>
        <img src="/w3images/img_app.jpg" class="w3-image w3-margin-top" width="335" height="471">
      </div>
    </div>
  </div>
</div>
